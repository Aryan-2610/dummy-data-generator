<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>dummy data</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-T3c6CoIiuLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPKM2HN"
          crossorigin="anonymous">
</head>
<body>
    <div class="container">
        <button type="button" class="btn btn-success">Generate Now</button>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-C6RzsynM9kWDrMNeT87bh950GNyZPhcNXj1NW7RuBCsyN/oRjpcV8Qyq46cDfL"
            crossorigin="anonymous"></script>
    <script>
        //select the button
        console.log("selecting button");
        document.querySelector(".container>button").addEventListener("click",async()=>{
            //when button is clicked fetch data using req /generate 
            console.log("fetching data");
            let a=await fetch("/generate");

            //convert it into json
            console.log("converting to json");
            let r=await a.json();

            //display the data to console
            console.log(r);


        })
    </script>
</body>
</html>
